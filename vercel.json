{
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "routes": [
        {
            "handle": "filesystem"
        },
        {
            "src": "/.*\\.jsx",
            "contentType": "text/javascript"
        },
        {
            "src": "/.*",
            "dest": "/index.html"
        }
    ]
}